{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.cs"
          ],
		  "exclude": [
		  "**/obj/**",
		  "**/bin/**",
            "_site/**",
			"Generic/HostChannel.cs",
			"Generic/NamespaceDoc.cs"],
			"src": "Kernel/Automating/.",
        }
      ],
      "dest": "api/kernel",
	  "memberLayout": "separatePages",
    },
	{
      "src": [
        {
          "files": [
            "*.csproj"
          ],
		  "exclude": [
		  "**/obj/**",
		  "**/bin/**",
            "_site/**"		],
			"src": "Cad/Design/Automating/.",
        }
      ],
      "dest": "api/cad",
	  "memberLayout": "separatePages",
    }
  ],
  "build": {
	  "globalMetadata": 
	{
      "_appTitle": "TopSolid Automation website",
      "_enableSearch": "true",
	  "_appLogoPath": "img/TopSolid_footer.svg",
      "_disableContribution": true

    }
	,
       "content": [
      {
        "files": [
          "api/kernel/**.yml"
        ],
		"src": "."
      },
	  {
        "files": [
          "api/cad/**.yml"
        ],
		"src": "."
      },

      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
		  "Kernel/Automating/**",
		  "Cad/Design/Automating/**"
        ],
		"src": "."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
		"src": "Kernel/Automating/."
      },
	  {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
		"src": "Cad/Design/Automating/."
      },
	  {
        "files": [
          "img/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
		"src": "."
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
		"src": "Kernel/Automating/."
      },
	  {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
		"src": "Cad/Design/Automating/."
      }
    ],
    "dest": "_site",
	"postProcessors": ["ExtractSearchIndex"],	
    "template": [
      "default",
		 "D:/testDOCFX/custom"
	]
  }
}